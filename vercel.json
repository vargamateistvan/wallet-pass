{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "installCommand": "yarn install",
  "buildCommand": "yarn workspace @wallet-pass/shared build && yarn workspace frontend build:vercel",
  "outputDirectory": "apps/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
