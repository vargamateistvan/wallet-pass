{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "yarn install",
  "buildCommand": "npx turbo build --filter=frontend...",
  "outputDirectory": "apps/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
