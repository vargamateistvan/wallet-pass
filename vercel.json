{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": null,
  "installCommand": "yarn install",
  "buildCommand": "npx turbo build --filter=@wallet-pass/shared && cd apps/frontend && yarn build:vercel && ls -la dist",
  "outputDirectory": "apps/frontend/dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
